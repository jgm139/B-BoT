<?xml version="1.0"?>
<package format="2">
  <name>pal_statsd_collector</name>
  <version>1.1.0</version>
  <description>Node that collects statistics, aggregates them and sends them to carbon</description>

  <maintainer email="jordan.palacios@pal-robotics.com">Jordan Palacios</maintainer>

  <license>LGPLv3</license>

  <author email="jordan.palacios@pal-robotics.com">Jordan Palacios</author>

  <buildtool_depend>catkin</buildtool_depend>

  <exec_depend>pal_statistics_msgs</exec_depend>
  <exec_depend>python-statsd</exec_depend>
  <exec_depend>rospy</exec_depend>

  <test_depend>rostest</test_depend>

</package>
